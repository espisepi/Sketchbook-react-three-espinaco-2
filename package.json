{
  "name": "sketchbook",
  "version": "0.4.0",
  "description": "3D playground built on three.js and cannon.js",
  "keywords": [
    "3d",
    "threejs",
    "cannonjs",
    "game",
    "engine",
    "typescript",
    "sketchbook"
  ],
  "main": "build/sketchbook.min.js",
  "types": "build/types/sketchbook.d.ts",
  "scripts": {
    "dev": "concurrently -k \"tsc -p ./src/server -w\" \"nodemon ./dist/server/server.js\" \"webpack serve --config webpack.dev.js\"",
    "build": "webpack --config webpack.prod.js",
    "start": "node ./dist/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swift502/Sketchbook.git"
  },
  "author": "swift502 <blaha.j502@gmail.com> (http://jblaha.art/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swift502/Sketchbook/issues"
  },
  "homepage": "https://github.com/swift502/Sketchbook",
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.112",
    "@types/node": "^17.0.41",
    "css-loader": "^3.6.0",
    "style-loader": "^1.3.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "nipplejs": "^0.10.1",
    "sweetalert2": "^9.17.2",
    "three": "0.113.0",
    "three-csm": "1.0.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3"
  }
}
